{
  "name": "nodejs-typescript-template",
  "version": "1.470.0",
  "description": "A template for a nodejs application, using typescript",
  "main": "dist/app.js",
  "author": "",
  "license": "MIT",
  "module": "dist/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src/**/*.ts --exec tsx ./src/app.ts",
    "dev:web": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch src/**/*.ts --exec tsx ./src/web-app.ts",
    "dev:client": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "vite build",
    "start": "node ./dist/app.js",
    "start:web": "node ./dist/web-app.js",
    "start:deno": "deno run --allow-read --allow-write --allow-net --allow-env --allow-sys --allow-run --unstable --watch src/app.ts",
    "lint": "eslint --fix --quiet src/**/*.ts",
    "format": "prettier --loglevel silent --write src/**/*.ts",
    "test": "mocha --require tsx/cjs --recursive 'test/**/*.test.ts'",
    "test:watch": "mocha --require tsx/cjs --recursive 'test/**/*.test.ts' --watch",
    "test:coverage": "nyc mocha --require tsx/cjs --recursive 'test/**/*.test.ts'",
    "test:unit": "mocha --require tsx/cjs --recursive 'test/unit/**/*.test.ts'",
    "test:integration": "mocha --require tsx/cjs --recursive 'test/integration/**/*.test.ts'"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.44.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "exif": "^0.6.0",
    "exif-parser": "^0.1.12",
    "exif-reader": "^2.0.2",
    "express": "^5.1.0",
    "icloudjs": "1.6.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "samsung-frame-connect": "^0.9.0",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/exif": "^0.6.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.16.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "chai": "^5.2.0",
    "deno": "^2.4.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint-parser": "^21.0.2"
  }
}
